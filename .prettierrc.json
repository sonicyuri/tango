{
    "trailingComma": "none",
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": true,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "preserve",
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "insertPragma": true,
    "endOfLine": "auto",
    "embeddedLanguageFormatting": "auto"
}
